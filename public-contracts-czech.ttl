# --- Vocabulary terms -----------------------------------------

# Druh řízení veřejné zakázky 
# 0..1
pccz:hasManagement a rdf:Property ;
  rdfs:domain pc:Contract ;
  rdfs:range pccz:Management ;
  rdfs:label "Druh řízení veřejné zakázky"@cs, "Kind of management of a public contract"@en ;
  rdfs:comment "Druh řízení veřejné zakázky"@cs, "Kind of management of a public contract"@en ;
  rdfs:isDefinedBy pccz: .

# Druh řízení
pccz:Management a rdfs:Class ;
  rdfs:range pccz:Management ;
  rdfs:label "Druh řízení"@cs, "Kind of management"@en ;
  rdfs:comment "Definuje povolené druhy řízení veřejných zakázek"@cs, "Defines allowed kinds of managament of public contracts"@en ;
  rdfs:isDefinedBy pccz: .
pccz:otevrene a pccz:Management .
pccz:uzsi a pccz:Management .
pccz:jednaciSUverejnenim a pccz:Management .
pccz:jednaciBezUverejneni a pccz:Management .
pccz:soutezniDialog a pccz:Management .
pccz:zjednodusenePodlimitni a pccz:Management .
  
# Limit veřejné zakázky 
# 0..1
pccz:hasLimit a rdf:Property ;
  rdfs:domain pc:Contract ;
  rdfs:range pccz:Limit ;
  rdfs:label "Limit veřejné zakázky"@cs, "Limit of a public contract"@en ;
  rdfs:comment "Limit veřejné zakázky"@cs, "Limit of a public contract"@en ;
  rdfs:isDefinedBy pccz: .

# Limit
pccz:Limit a rdfs:Class ;
  rdfs:label "Limit"@cs, "Limit"@en ;
  rdfs:comment "Definuje povolené limity veřejných zakázek"@cs, "Defines allowed limits of public contracts"@en ;
  rdfs:isDefinedBy pccz: .
pccz:malehoRozsahu a pccz:Limit .
pccz:podlimitni a pccz:Limit .
pccz:nadlimitni a pccz:Limit .
  
# Druh veřejné zakázky 
# 0..1
pccz:hasKind a rdf:Property ;
  rdfs:domain pc:Contract ;
  rdfs:range pccz:Kind ;
  rdfs:label "Druh veřejné zakázky"@cs, "Kind of a public contract"@en ;
  rdfs:comment "Druh veřejné zakázky"@cs, "Kind of a public contract"@en ;
  rdfs:isDefinedBy pccz: .
  
# Druh
pccz:Kind a rdfs:Class ;
  rdfs:label "Druh"@cs, "Druh"@en ;
  rdfs:comment "Definuje povolené druhy veřejných zakázek"@cs, "Defines allowed kinds of public contracts"@en ;
  rdfs:isDefinedBy pccz: .
pccz:dodavky a pccz:Kind .
pccz:stavebniPrace a pccz:Kind .
pccz:sluzby a pccz:Kind .