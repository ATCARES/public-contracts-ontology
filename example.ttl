@prefix dc:         <http://purl.org/dc/terms/> .
@prefix gr:         <http://purl.org/goodrelations/v1#> .
@prefix payment:    <http://reference.data.gov.uk/def/payment#> .
@prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:        <http://www.w3.org/2001/XMLSchema#> .

# --- Own namespaces -----------------------------------------

@prefix pc:          <http://purl.org/opendata-cz/public-contracts#> .
@prefix cex:        <http://example.org/cex> .

cex:contract1	a pc:Contract ;
  dc:title "Jezdecké potřeby městské policie"@cs ;
  pc:subject "18911000"^^xsd:string, "18913000"^^xsd:string ;
  dc:issued "2010-01-01"^^xsd:date ;
  pc:bidDeadline "2010-02-01"^^xsd:date ;
  pc:numberOfBids "10"^^xsd:int ;
  pc:bid cex:bid1, cex:bid2 ;
  pc:acceptedBid cex:bid1 ;
  pc:contractPrice [
   	a gr:PriceSpecification ;
	  gr:valueAddedTaxIncluded "false"^^xsd:boolean ;
   	gr:hasCurrency "CZK" ;
	  gr:hasCurrencyValue "1000000.0"^^xsd:float
  ] ;
  pc:supplementPrice [
    a gr:PriceSpecification ;
    gr:hasCurrency "CZK" ;
    gr:hasCurrencyValue "300000.0"^^xsd:float
  ] ;
  pc:subsidy [
    a gr:PriceSpecification ;
    gr:hasCurrency "EUR" ;
    gr:hasCurrencyValue "10000.0"^^xsd:float
  ] ;
  pc:quality "true"^^xsd:boolean ;
  pc:onSchedule "false"^^xsd:boolean ;
  pc:url <http://www.mesto.cz/...html> ;
  pc:document <http://www.mesto.cz/...pdf> ;
  pc:payment cex:payment1, cex:payment2, cex:payment3
  .

cex:bid1 a pc:Bid ;
   pc:bidder cex:organization1 ;
   pc:bidPrice [
     a gr:PriceSpecification ;
     gr:hasCurrency "CZK" ;
     gr:hasCurrencyValue "1000000.0"^^xsd:float
   ].

cex:bid2 a pc:Bid ;
  pc:bidder cex:organization2 ;
  pc:bidPrice [
    a gr:priceSpecification ;
    gr:hasCurrency "CZK" ;
    gr:hasCurrencyValue "1500000.0"^^xsd:float
  ]
  .

cex:organization1 a gr:BusinessEntity;
  pc:orgNumber "1234567" ;
  gr:legalName "První koňařská a.s."
  .

cex:organization2 a gr:BusinessEntity;
  pc:orgNumber "789654123" ;
  gr:legalName "První jezdecká a.s."
  .

cex:payment1 a payment:Payment;
  # jdou i řádky faktur
  payment:payer cex:organization1 ;
  payment:payee cex:organization2
  .

cex:payment2  a payment:Payment;
  # jdou i řádky faktur
  payment:payer cex:organization1 ;
  payment:payee  cex:organization2
  .

cex:payment3  a payment:Payment;
  # jdou i řádky faktur
  payment:payer cex:organization1 ;
  payment:payee  cex:organization2
  .
